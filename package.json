{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "update": "git submodule foreach git pull origin master && ./node_modules/.bin/hexo clean && ./node_modules/.bin/hexo g -d",
    "deploy": "./node_modules/.bin/hexo clean && ./node_modules/.bin/hexo g -d",
    "server": "hexo s --debug",
    "new": "node new.js",
    "start":"npm run new",
    "build": "hexo clean && hexo g",
    "gen": "./node_modules/.bin/hexo clean && ./node_modules/.bin/hexo g"
  },
  "hexo": {
    "version": "3.7.1"
  },
  "dependencies": {
    "hexo": "^3.7.1",
    "hexo-all-minifier": "^0.5.2",
    "hexo-autonofollow": "^1.0.1",
    "hexo-deployer-git": "^0.3.1",
    "hexo-fs": "^0.2.3",
    "hexo-generator-archive": "^0.1.5",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-feed": "^1.2.2",
    "hexo-generator-index": "^0.2.1",
    "hexo-generator-sitemap": "^1.2.0",
    "hexo-generator-tag": "^0.2.0",
    "hexo-renderer-ejs": "^0.3.1",
    "hexo-renderer-marked": "^0.3.2",
    "hexo-renderer-stylus": "^0.3.3",
    "hexo-server": "^0.3.3",
    "pretty": "^2.0.0",
    "qcloud-cdn-node-sdk": "^1.0.0"
  }
}